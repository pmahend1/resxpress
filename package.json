{
    "name": "resxpress",
    "displayName": "ResXpress",
    "description": "Resources Express for resx files",
    "publisher": "PrateekMahendrakar",
    "icon": "images/logo.png",
    "repository": {
        "url": "https://github.com/pmahend1/resxpress.git"
    },
    "version": "1.0.2",
    "engines": {
        "vscode": "^1.40.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:resxpress.resxpreview"
    ],
    "main": "./dist/extension",
    "contributes": {
        "commands": [{
            "command": "resxpress.resxpreview",
            "title": "ResXpress: Preview Resx"
        }],
        "configuration": {
            "title": "ResXpress"
        },
        "languages": [{
            "id": "xml",
            "extensions": [
                ".resx"
            ]
        }]
    },
    "scripts": {
        "vscode:prepublish": "webpack --mode production",
        "webpack": "webpack --mode development",
        "webpack-dev": "webpack --mode development --watch",
        "test-compile": "tsc -p ./"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.40.0",
        "glob": "^7.1.5",
        "mocha": "^6.2.2",
        "ts-loader": "^6.2.1",
        "tslint": "^5.20.0",
        "typescript": "^3.6.4",
        "vscode-test": "^1.2.2",
        "webpack": "^4.41.4",
        "webpack-cli": "^3.3.10"
    },
    "dependencies": {
        "resx-parser": "^1.0.1"
    }
}